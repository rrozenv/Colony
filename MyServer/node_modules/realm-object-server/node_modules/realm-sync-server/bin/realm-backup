#!/bin/sh

script_dir=$(cd $(dirname "$0") && pwd)
link=$(readlink "$0")
if [ "$link" != "" ]; then
        # original is a symlink
        real_script="$script_dir"/$(readlink "$0")
else
        real_script="$0"
fi

script_dir=$(dirname "$real_script")
arch_name=$(node -e 'console.log(`${process.platform}-${process.arch}`);')
arch_dir="$script_dir"/../compiled/$arch_name
arch_file="$arch_dir"/realm-backup

exec "$arch_file" $@